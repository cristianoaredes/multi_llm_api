SERVER_PORT=8081
LOG_LEVEL=INFO # Options: ALL, FINEST, FINER, FINE, CONFIG, INFO, WARNING, SEVERE, SHOUT, OFF

# IMPORTANTE: Substitua pelo seu valor real antes de executar o servidor
# Obtenha em: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Gemini Configuration
GEMINI_MODEL=gemini-1.5-flash-latest # Options: gemini-1.5-flash-latest, gemini-1.5-pro-latest, gemini-1.0-pro, etc.
GEMINI_MAX_TOKENS=2048
GEMINI_TEMPERATURE=0.7

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=api_dart
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_USE_SSL=false

# JWT Configuration
JWT_SECRET=YOUR_SECRET_KEY_HERE
JWT_EXPIRATION_HOURS=24

# AI Provider (gemini or openrouter)
AI_PROVIDER=gemini

# OpenRouter Configuration
OPENROUTER_API_KEY=YOUR_API_KEY_HERE
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=openai/gpt-3.5-turbo
OPENROUTER_MAX_TOKENS=2048
OPENROUTER_TEMPERATURE=0.7
OPENROUTER_ENABLE_STREAMING=false
